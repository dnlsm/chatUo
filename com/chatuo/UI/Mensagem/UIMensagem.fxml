<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <BorderPane style="-fx-border-radius: 20; -fx-border-width: 1; -fx-border-style: dashed inside; -fx-border-color: #505050; -fx-background-radius: 20; -fx-background-color: #CCCCCC;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <VBox BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="contactName" text="Name" textFill="#005000" VBox.vgrow="ALWAYS">
                     <font>
                        <Font name="Calibri" size="16.0" />
                     </font>
                  </Label>
                  <Label fx:id="msgText" maxWidth="500.0" text="MsgText" wrapText="true">
                     <font>
                        <Font name="Calibri" size="13.0" />
                     </font></Label>
                  <BorderPane>
                     <right>
                        <Label fx:id="msgTime" text="msgTime" textFill="#606060" wrapText="true" BorderPane.alignment="CENTER">
                           <font>
                              <Font name="Arial" size="12.0" />
                           </font></Label>
                     </right>
                  </BorderPane>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
         </center>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </BorderPane>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</AnchorPane>
